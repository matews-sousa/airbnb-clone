<%= render "shared/profile_nav" %>
<header class="flex items-center justify-between">
  <h1 class="text-lg font-semibold">My Favorites</h1>
</header>
<% if @favorites.empty? %>
  <p class="text-gray-600 mt-6">You haven't favorited any place yet.</p>
<% else %>
  <div class="grid grid-cols-1 gap-6 mt-6">
    <% @favorites.each do |place| %>
      <%= render "places/place", place: place %>
    <% end %>
  </div>
<% end %>
